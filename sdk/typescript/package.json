{
  "name": "@aragora/sdk",
  "version": "2.7.4",
  "description": "Official TypeScript SDK for Aragora multi-agent debate platform",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "generate:types": "python ../../scripts/generate_sdk_types.py --output src/openapi-types.ts",
    "check:types": "python ../../scripts/generate_sdk_types.py --output src/openapi-types.ts --check",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "aragora",
    "ai",
    "debate",
    "multi-agent",
    "llm",
    "sdk"
  ],
  "author": "Aragora Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/an0mium/aragora.git",
    "directory": "sdk/typescript"
  },
  "homepage": "https://github.com/an0mium/aragora/blob/main/docs/START_HERE.md",
  "bugs": {
    "url": "https://github.com/an0mium/aragora/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "typescript-eslint": "^8.56.0",
    "vitest": "^4.0.18"
  },
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "peerDependenciesMeta": {
    "ws": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}

# Aragora API Configuration
# Copy this file to .env.local and fill in your values

# Server-side environment variables (private, not exposed to browser)
ARAGORA_API_URL=http://localhost:8080
ARAGORA_API_KEY=your-api-key-here

# Client-side environment variables (public, exposed to browser)
# These must be prefixed with NEXT_PUBLIC_
NEXT_PUBLIC_ARAGORA_API_URL=http://localhost:8080
NEXT_PUBLIC_ARAGORA_API_KEY=your-api-key-here

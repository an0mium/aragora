      "id": 300,
      "title": "Circuit Breaker State",
      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "$datasource" },
      "description": "0=closed (healthy), 1=open (tripping), 2=half-open (recovering)",
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [
            { "options": { "0": { "color": "green", "text": "CLOSED" }, "1": { "color": "red", "text": "OPEN" }, "2": { "color": "yellow", "text": "HALF-OPEN" } }, "type": "value" }
          ],
          "thresholds": {
            "steps": [
              { "color": "green", "value": null },
              { "color": "red", "value": 1 }
            ]
          }
        }
      },
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 1 },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "textMode": "auto"
      },
      "targets": [
        {
          "expr": "aragora_agent_circuit_breaker_state{namespace=~\"$namespace\"}",
          "legendFormat": "{{agent_type}}",
          "refId": "A"
        }
      ],
      "title": "Agent Circuit Breaker States",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "$datasource" },
      "description": "Cross-pollination and gateway circuit breaker states",

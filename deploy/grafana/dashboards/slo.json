      "targets": [
        {
          "expr": "(sum(rate(aragora_http_request_duration_seconds_bucket{le=\"0.5\",namespace=~\"$namespace\"}[5m])) + sum(rate(aragora_http_request_duration_seconds_bucket{le=\"2.0\",namespace=~\"$namespace\"}[5m])) - sum(rate(aragora_http_request_duration_seconds_bucket{le=\"0.5\",namespace=~\"$namespace\"}[5m]))) / 2 / sum(rate(aragora_http_request_duration_seconds_count{namespace=~\"$namespace\"}[5m]))",
          "legendFormat": "HTTP Apdex",
          "refId": "A"
